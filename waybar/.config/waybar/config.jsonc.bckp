// -*- mode: jsonc -*-
[
{
    "output": "HDMI-A-1",
    "height": 30,
    "spacing": 4,
    "modules-left": [
        "hyprland/workspaces",
        "hyprland/window",
    ],
    "modules-center": [
        "clock",
    ],
    "modules-right": [
        "hyprland/language",
        "tray",
        "idle_inhibitor",
        "pulseaudio",
        "power-profiles-daemon",
        "custom/hdr-switch",
        "temperature",
        "battery",
